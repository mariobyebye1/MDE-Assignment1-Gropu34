<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="nlToSql" nsURI="http://www.ac.uk/kcl/inf/NlToSql" nsPrefix="nlToSql">
  <eClassifiers xsi:type="ecore:EClass" name="AccountingSpeech">
    <eStructuralFeatures xsi:type="ecore:EReference" name="statements" upperBound="-1"
        eType="#//Statement" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Statement"/>
  <eClassifiers xsi:type="ecore:EClass" name="CreateTableStatement" eSuperTypes="#//Statement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="table" eType="#//Table"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="columns" eType="#//ColumnList"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ColumnList">
    <eStructuralFeatures xsi:type="ecore:EReference" name="columnItem" upperBound="-1"
        eType="#//Column" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Table">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Column">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="#//Datatype"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="table" eType="#//Table"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="Datatype">
    <eLiterals name="integer" literal="integer"/>
    <eLiterals name="string" value="1" literal="string"/>
    <eLiterals name="date" value="2" literal="date"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SelectStatement" eSuperTypes="#//Statement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="tables" eType="#//SelectTablesList"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="columns" eType="#//SelectColumnsList"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="condition" eType="#//LogicExpressions"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="group" eType="#//SelectColumnsList"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="InsertStatement" eSuperTypes="#//Statement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="table" eType="#//SelectTable"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="columns" eType="#//SelectColumnsList"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="values" eType="#//InserValues"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="UpdateStatement" eSuperTypes="#//Statement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="table" eType="#//SelectTable"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="updates" eType="#//SelectUpdateList"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="condition" eType="#//LogicExpressions"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DeleteStatement" eSuperTypes="#//Statement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="tables" eType="#//SelectTablesList"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SelectUpdateList">
    <eStructuralFeatures xsi:type="ecore:EReference" name="updateItem" upperBound="-1"
        eType="#//UpdateItem" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="UpdateItem">
    <eStructuralFeatures xsi:type="ecore:EReference" name="column" eType="#//ColumnReference"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="InserValues">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="valueList" unique="false"
        upperBound="-1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SelectTablesList">
    <eStructuralFeatures xsi:type="ecore:EReference" name="selectTable" unique="false"
        upperBound="-1" eType="#//Table"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SelectTable" eSuperTypes="#//DeleteStatement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="condition" eType="#//LogicExpressions"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="table" eType="#//Table"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SelectColumnsList">
    <eStructuralFeatures xsi:type="ecore:EReference" name="selectColumn" upperBound="-1"
        eType="#//ColumnReference" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ColumnReference">
    <eStructuralFeatures xsi:type="ecore:EReference" name="column" eType="#//Column"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="table" eType="#//Table"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="LogicExpressions"/>
  <eClassifiers xsi:type="ecore:EClass" name="Comparison" eSuperTypes="#//LogicExpressions">
    <eStructuralFeatures xsi:type="ecore:EReference" name="leftHandSide" eType="#//Column"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="operator" eType="#//ComparisonOperatorString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="rightHandSide" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ComparisonOperatorString">
    <eLiterals name="EQUAL_TO" literal="is equal to"/>
    <eLiterals name="NOT_EQUAL_TO" value="1" literal="is not equal to"/>
    <eLiterals name="LESS_THAN" value="2" literal="less than"/>
    <eLiterals name="GREATER_THAN" value="3" literal="greater than"/>
    <eLiterals name="LESS_THAN_OR_EQUAL_TO" value="4" literal="less than or equal to"/>
    <eLiterals name="GREATER_THEN_OR_EQUAL_TO" value="5" literal="greater than or equal to"/>
    <eLiterals name="EQUAL_TO_SIGN" value="6" literal="="/>
    <eLiterals name="NOT_EQUAL_TO_SIGN" value="7" literal="&lt;>"/>
    <eLiterals name="LESS_THAN_SIGN" value="8" literal="&lt;"/>
    <eLiterals name="GREATER_THAN_SIGN" value="9" literal=">"/>
    <eLiterals name="LESS_THAN_OR_EQUAL_TO_SIGN" value="10" literal="&lt;="/>
    <eLiterals name="GREATER_THEN_OR_EQUAL_TO_SIGN" value="11" literal=">="/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Condition" eSuperTypes="#//LogicExpressions">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" eType="#//LogicExpressions"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="logicOperator" unique="false"
        upperBound="-1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" upperBound="-1"
        eType="#//Comparison" containment="true"/>
  </eClassifiers>
</ecore:EPackage>
